{
    "name": "shopsys/http-smoke-testing",
    "type": "library",
    "description": "HTTP smoke test case for testing all configured routes in your Symfony project",
    "keywords": ["smoke testing", "testing", "Symfony", "routing", "PHPUnit"],
    "license": "MIT",
    "authors": [
        {
            "name": "Shopsys",
            "homepage": "https://www.shopsys.com/"
        }
    ],
    "autoload": {
        "psr-4": {
            "Shopsys\\HttpSmokeTesting\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\HttpSmokeTesting\\": "tests/"
        }
    },
    "require": {
        "php": "^8.3",
        "phpunit/phpunit": "^11.2.1",
        "symfony/framework-bundle": "^6.4",
        "symfony/http-foundation": "^6.4",
        "symfony/dependency-injection": "^6.4",
        "symfony/routing": "^6.4",
        "doctrine/annotations": "^1.10.4"
    }
}
